{
    "name": "misterspelik/yii2-mpdf",
    "description": "A Yii2 wrapper component for the mPDF library which generates PDF files from UTF-8 encoded HTML.",
    "keywords": [
        "yii2",
        "extension",
        "pdf",
        "mpdf",
        "utf8",
        "html",
        "component"
    ],
    "homepage": "https://github.com/misterspelik/yii2-mpdf",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Yaroslav Krutikov",
            "email": "misterspelik@gmail.com",
            "homepage": "https://github.com/misterspelik"
        }
    ],
    "require": {
        "php": ">=7.1",
        "misterspelik/mpdf": "^8.1.8"
    },
    "autoload": {
        "psr-4": {
            "misterspelik\\mpdf\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
